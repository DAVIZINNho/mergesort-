#include <bits/stdc++.h>

using namespace std;

void merge(int *vet,int pi,int pf,int M)    //merge
{
	int *aux,k=0;                              
	int i = pi, j = M+1;
	aux = new int[pf];                 //alocação dinamic
	while(i<=M && j<=pf)
	{
		if(vet[i] <= vet[j]) 
		{
			aux[k] = vet[i];
			i++;
		}
		else{ 
			aux[k] = vet[j];
			j++;
		}
		k++;
	}
	while(i<=M)
	{
		aux[k] = vet[i];
		k++;
		i++;
	}
	while(j<=pf)
	{
		aux[k] = vet[j];
		k++;
		j++;
	}
	for(i=pi;i<=pf;i++)
	{
		vet[i] = aux[i-pi];  //como ele faz parte por parte tenho que colocar na pos-i exemplo fiz a parte esqueda meu pi = 3 e meu i tbm que da 0 no vetor aux e passa na ps 3 do principal
	}
	delete aux;
}

void mergesort(int *vet, int pi,int pf)
{
	int meio;
	if(pi<pf)
	{
		meio = (pi+pf)/2;
		mergesort(vet,pi,meio);
		mergesort(vet,meio+1,pf);
		merge(vet,pi,pf,meio);
	}
}
int main()
{
	int vetor[5];
	for(int i=0;i<5;i++)
	{
		cin >> vetor[i];
	}
	mergesort(vetor,0,4);
	for(int i=0;i<5;i++)
	{
		cout << vetor[i]; 
	}
}
